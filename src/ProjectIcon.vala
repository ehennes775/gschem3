namespace Geda3
{
    /**
     * An enum identifying icons to use in the poject tree
     */
    public enum ProjectIcon
    {
        /**
         * Indicates a blank or unknown document
         *
         * {{Blank.svg}}
         */
        BLANK,

        /**
         * A blue folder
         *
         * {{BlueFolder.svg}}
         */
        BLUE_FOLDER,

        /**
         * A missing document
         *
         * {{Missing.svg}}
         */
        MISSING,

        /**
         * An orange folder
         *
         * {{OrangeFolder.svg}}
         */
        ORANGE_FOLDER,

        /**
         * An schematic file
         *
         * {{Schematic.svg}}
         */
        SCHEMATIC,

        /**
         * A symbol file
         *
         * {{Symbol.svg}}
         */
        SYMBOL,

        /**
         * The number of icons available
         */
        COUNT;
    }
}
