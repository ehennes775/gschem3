namespace Geda3
{
    /**
     * An enum identifying icons to use in the poject tree
     */
    public enum ProjectIcon
    {
        /**
         * Indicates a blank or unknown document
         *
         * {{Blank.svg}}
         */
        BLANK,

        /**
         * A blue folder
         *
         * {{BlueFolder.svg}}
         */
        BLUE_FOLDER,

        /**
         * A green folder
         *
         * {{GreenFolder.svg}}
         */
        GREEN_FOLDER,

        /**
         * A missing document
         *
         * {{Missing.svg}}
         */
        MISSING,

        /**
         * An orange folder
         *
         * {{OrangeFolder.svg}}
         */
        ORANGE_FOLDER,

        /**
         * A plum folder
         *
         * {{PlumFolder.svg}}
         */
        PLUM_FOLDER,

        /**
         * A red folder
         *
         * {{RedFolder.svg}}
         */
        RED_FOLDER,

        /**
         * An schematic file
         *
         * {{Schematic.svg}}
         */
        SCHEMATIC,

        /**
         * A symbol file
         *
         * {{Symbol.svg}}
         */
        SYMBOL,

        /**
         * A yellow folder
         *
         * {{YellowFolder.svg}}
         */
        YELLOW_FOLDER,

        /**
         * The number of icons available
         */
        COUNT;
    }
}
