<?xml version="1.0" encoding="UTF-8" ?>
<interface>

    <object class="GtkListStore" id="fill-type-list">
        <columns>
            <column type="gchararray"/>
            <column type="gint"/>
        </columns>
        <data>
            <row>
                <col id="0">Hollow</col>
                <col id="1">0</col>
            </row>
            <row>
                <col id="0">Filled</col>
                <col id="1">1</col>
            </row>
            <row>
                <col id="0">Mesh</col>
                <col id="1">2</col>
            </row>
            <row>
                <col id="0">Hatch</col>
                <col id="1">3</col>
            </row>
        </data>
    </object>

    <object class="GtkListStore" id="width-list">
        <columns>
            <column type="gchararray"/>
