            </row>
        </data>
    </object>

    <object class="GtkListStore" id="pitch-list">
        <columns>
            <column type="gchararray"/>
        </columns>
        <data>
            <row>
                <col id="0">50</col>
            </row>
            <row>
                <col id="0">100</col>
