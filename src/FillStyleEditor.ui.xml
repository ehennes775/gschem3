                <col id="0">10</col>
            </row>
            <row>
                <col id="0">15</col>
            </row>
            <row>
                <col id="0">20</col>
            </row>
        </data>
    </object>

    <object class="GtkListStore" id="angle-list">
        <columns>
            <column type="gchararray"/>
        </columns>
        <data>
            <row>
                <col id="0">0</col>
            </row>
            <row>
                <col id="0">30</col>
            </row>
            <row>
                <col id="0">45</col>
            </row>
            <row>
                <col id="0">60</col>
