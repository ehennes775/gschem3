                        <property name="height">1</property>
                    </packing>
                </child>
                <child>
                    <object class="GtkComboBox" id="fill-pitch-2-combo">
                        <property name="entry-text-column">0</property>
                        <property name="has-entry">true</property>
                        <property name="hexpand">true</property>
